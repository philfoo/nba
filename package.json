{
  "name": "nba",
  "version": "1.0.0",
  "description": "NBA stats application",
  "main": "index.js",
  "babel": {
    "presets": [
      "react",
      "es2015"
    ],
    "compact": false,
    "ignore": [
      "**/node_modules/**",
      "**/client/react/**"
    ]
  },
  "repository": {
